{
  "name": "bi",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "func",
    "cli",
    "shared",
    "express"
  ],
  "scripts": {
    "build": "run-s build:shared build:func build:express",
    "dev": "run-p dev:*",
    "build:shared": "npm -w shared run build",
    "build:func": "npm -w func run build",
    "build:express": "npm -w express run build",
    "dev:shared": "npm -w shared run dev",
    "dev:func": "npm -w func run dev",
    "dev:express": "npm -w express run dev"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
